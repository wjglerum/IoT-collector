{
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "grafana.dcos.lunatech.com"
  },
  "id": "/grafana",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 3000,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/grafana:3000"
        },
        "protocol": "tcp",
        "servicePort": 10101,
        "name": "https"
      }
    ],
    "type": "MESOS",
    "volumes": [],
    "docker": {
      "image": "grafana/grafana",
      "forcePullImage": false,
      "parameters": []
    }
  },
  "cpus": 2,
  "disk": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "MESOS_HTTP",
      "path": "/"
    }
  ],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 2056,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "constraints": []
}